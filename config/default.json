    "log4js": {
        "appenders": {
            "file": {
                "type": "dateFile",
                "filename": "./logs/logger.log",
                "pattern": ".yyyy-MM-dd",
                "daysToKeep": 30,
                "compress": true,
                "layout": {
                    "type": "pattern",
                    "pattern": "%d{yyyy-MM-dd hh:mm:ss.SSS} PID[%z] %[%p%] %c: %m"
                }
            },
            "console": {
                "type": "console",
                "layout": {
                    "type": "pattern",
                    "pattern": "%d{yyyy-MM-dd hh:mm:ss.SSS} PID[%z] %[%p%] %c: %m"
                }
            }
        },
        "categories": {
            "default": {
                "appenders": [
                    "file",
                    "console"
                ],
                "level": "debug"
            }
        },
        "pm2": true,
        "pm2InstanceVar": "INSTANCE_ID"
    }
}